        <api_name>x_1589793_new_empl.GetDeptName</api_name>
        <caller_access/>
        <client_callable>true</client_callable>
        <description>Script Include to return the department name.  The calling client-side script passes a departmend head name for a Department Head table record.</description>
        <mobile_callable>false</mobile_callable>
        <name>GetDeptName</name>
        <sandbox_callable>true</sandbox_callable>
        <script><![CDATA[var GetDeptName = Class.create();
// Extend the global.AbstractAjaxProcessor class
GetDeptName.prototype = Object.extendsObject(global.AbstractAjaxProcessor,{
    getDept: function() {
        var deptHeadRecord = new GlideRecord("x_1589793_new_empl_department_head");
		gs.info("RECCC", deptHeadRecord);
		deptHeadRecord.get(this.getParameter('sysparm_department_head'));
        return deptHeadRecord.department + ''; 
    },
    type: 'GetDeptName'
});]]></script>
